package com.coforge.training.springcore;

import org.springframework.context.ApplicationContext;
import org.springframework.context.annotation.AnnotationConfigApplicationContext;

import com.coforge.training.springcore.config.ProductConfig;
import com.coforge.training.springcore.model.Employee1;

/**
* Author :Vivek.Patel
* Date   :Jul 30, 2025
* Time   :11:35:50â€¯AM
* Project:spring-core
*/

public class Employee1App {

	public static void main(String[] args) {
        ApplicationContext context=new AnnotationConfigApplicationContext(ProductConfig.class);
        Employee1 emp1=(Employee1)context.getBean("employee1",Employee1.class);
        emp1.displayEmployeeInfo();
        System.out.println();
        Employee1 emp2=(Employee1)context.getBean("employee2",Employee1.class);
        emp2.displayEmployeeInfo();
        ((AnnotationConfigApplicationContext)context).close();

	}

}
