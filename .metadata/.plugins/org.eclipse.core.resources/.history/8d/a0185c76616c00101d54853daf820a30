    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
        
        <!-- Create Student Objects. IOC - Creation of Objects is out sourced to 
		XML file. -->
	<bean id="student1" class="com.coforge.training.springcore.model.Student">
		<!-- Dependency Injection using Setter -->
		<property name="rollNo" value="999"></property>
		<property name="name" value="Alex John"></property>
		<property name="college" value="IP University"></property>
		<property name="marks" value="85"></property>
	</bean>
	
	<bean id="student2" class="com.wipro.training.springcore.model.Student">
		<!-- Dependency Injection using Setter -->
		<property name="rollNo" value="777"></property>
		<property name="name" value="Mary Kom"></property>
		<property name="college" value="VTU"></property>
		<property name="marks" value="92"></property>
	</bean>
